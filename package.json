    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@tailwindcss/postcss7-compat": "^2.2.7",
    "autoprefixer": "^9.8.6",
    "axios": "^0.21.1",
    "eslint": "^7.32.0",
    "fs": "^0.0.1-security",
    "less-vars-to-js": "^1.3.0",
    "mobx": "^6.3.2",
    "path": "^0.12.7",
    "postcss": "^7.0.36",
    "postcss-import": "^12.0.1",
    "react": "17.x",
    "react-dom": "17.x",
    "react-router-dom": "^5.2.0",
    "swr": "^0.5.6",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "umi": "^3.5.14",
    "vditor": "^3.8.6"
  },
  "devDependencies": {
    "@types/jest": "^27.0.0",
    "@types/node": "^16.4.13",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.14",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.3.5",
    "yorkie": "^2.0.0"
  }
